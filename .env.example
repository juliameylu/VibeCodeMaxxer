VITE_DEMO_MODE=true
# For live APIs in local dev/preview:
# Set VITE_DEMO_MODE=false and provide server-side env values below.
YELP_API_KEY=your_yelp_api_key_here
YELP_LOCATION=San Luis Obispo, CA
CALPOLY_EVENTS_SOURCE=https://r.jina.ai/http://now.calpoly.edu/

# Demo calling (set real values in .env.local only)
# E.164 format, e.g. +15551234567
DEMO_CALLER_NUMBER=+15550001111
DEMO_TARGET_NUMBER=+15550002222

# AWS multi-user backend
# session_or_cognito: supports existing x-session-token and Cognito bearer JWT
# cognito_only: only bearer JWT
# session: only x-session-token
AUTH_MODE=session_or_cognito
AWS_REGION=us-west-2
AWS_DYNAMO_TABLE=polyjarvis-state
AWS_STATE_PK=planner_state
AWS_STATE_SK=v1
COGNITO_USER_POOL_ID=us-west-2_example
COGNITO_APP_CLIENT_ID=exampleclientid
SNAPSHOT_FLUSH_MS=2000
